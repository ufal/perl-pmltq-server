% layout 'default';
% title 'PMLTQ Server - Add new treebank';


<h1>Add new treebank</h1>
%= form_for '/admin/treebank/add' => (method => 'POST') => begin
  %= label_for name => 'Treebank'
  %= text_field 'name'
  %for my $ch (qw/visible public anonaccess/) {
     %= label_for $ch => $ch
     %= check_box $ch => 1
  % }
 
  
  
  %= submit_button 'Add'
  
% end
